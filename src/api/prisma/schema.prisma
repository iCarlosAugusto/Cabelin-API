datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Client {
  id          String   @id @default(uuid())
  createdAt   DateTime @default(now())
  email       String   @unique
  phoneNumber String   @unique
  name        String
  password    String
}

model Partner {
  id          String   @id @default(uuid())
  createdAt   DateTime @default(now())
  email       String   @unique
  phoneNumber String   @unique
  name        String
  password    String
}
